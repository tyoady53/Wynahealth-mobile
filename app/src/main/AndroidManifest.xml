<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wynacom.wynahealth">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".DB_Local.GlobalVariable"
        android:allowBackup="false"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wynahealth.NoActionBar"
        android:usesCleartextTraffic="true"
        tools:replace="android:usesCleartextTraffic">
        <activity
            android:name=".transaction.NewOrderActivity"
            android:exported="false"
            android:label="@string/label_choose_patient"
            android:theme="@style/Theme.Wynahealth"/>
        <activity
            android:name=".CreateOrderActivity"
            android:exported="false"
            android:label="@string/title_activity_create_order" />
        <activity android:name=".DB_Local.CurrentOrder" />
        <activity
            android:name=".transaction.OrderConfirmationActivity"
            android:exported="false" />
        <activity
            android:name=".transaction.SelectProductActivity"
            android:exported="false"
            android:label="@string/label_choose_test"
            android:theme="@style/Theme.Wynahealth"/>
        <activity
            android:name=".EmailVerificationActivity"
            android:exported="false"/>
        <activity
            android:name=".OrderQRActivity"
            android:exported="false"/>
        <activity
            android:name=".transaction.CartsActivity"
            android:exported="false"
            android:theme="@style/Theme.Wynahealth"/>
        <activity
            android:name=".transaction.OrderActivity"
            android:exported="false"
            android:label="Order"
            android:theme="@style/Theme.Wynahealth"/>
        <activity
            android:name=".RegisterActivity"
            android:exported="true"/>
        <activity
            android:name=".Login_Activity"
            android:exported="true"/>
        <activity
            android:name=".LandingPage"
            android:exported="true"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Wynahealth"/>
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:label="@string/app_name"
            android:supportsRtl="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
